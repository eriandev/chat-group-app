{
    "tabWidth": 4,
    "semi": true,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "all",
    "svelteStrictMode": false,
    "svelteBracketNewLine": false,
    "plugins": ["prettier-plugin-svelte"],
    "svelteSortOrder" : "scripts-markup-styles",
    "overrides": [
        {
            "files": "*.ts",
            "options": {
                "parser": "typescript"
            }
        }
    ]
}